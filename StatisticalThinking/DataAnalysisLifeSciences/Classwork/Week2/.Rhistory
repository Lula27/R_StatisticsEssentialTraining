ps <- c(0.5, 0.5, 0.01, 0.01) # combination of  possible probabilities
ns <- c(5, 30, 30, 100) # combinations of n events
mypar(4,2)
for(i in 1:4) {
p <- ps[i]
sides <- 1/p
n <- ns[i]
zs <- replicate(10000, {
x <- sample(1:sides, n, replace=TRUE)
mean(x==1) - p / sqrt(p*(1-p)/n)
})
hist(zs, nclass=7)
qqnorm(zs)
abline(0,1)
}
